package ex1;

public class Semployee extends Employee {

	float monthlySalary = getbSalary() + (getHour()*20);
	float annuallySaraly = getbSalary()*5/100 * getExperience();

	public Semployee(String ID, String name, float bSalary, float monthlySalary, float annuallySaraly, int experience, int hour) {
		super(ID, name, bSalary, monthlySalary, annuallySaraly, experience, hour);
		// TODO Auto-generated constructor stub
		setMonthlySalary(monthlySalary);
		setAnnuallySaraly(annuallySaraly);
	}

}
