package ex1;

public class Semployee extends Employee {

	float monthlySalary = getbSalary() *20;
	float annuallySaraly = getbSalary()*5/100 * getExperience();

	public Semployee(String Stype, String ID, String name, float bSalary, int experience) {
		super(Stype, ID, name, bSalary, experience);
		// TODO Auto-generated constructor stub
	}

	public float monthlysalary(int hour) {
		// TODO Auto-generated method stub
		return getbSalary()*(hour*20);
	}

}
