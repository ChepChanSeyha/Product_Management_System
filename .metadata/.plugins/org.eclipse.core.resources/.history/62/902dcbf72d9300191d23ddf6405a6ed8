package staff;

public class Staff {
	
	private int id;
	private String username;
	private String password;
	private String position;
	private String contact;
	private double salary;
	private String gender;
	
	public Staff() {
		super();
	}
	public Staff(int id, String username, String password, String position, String contact, double salary, String gender) {
		super();
		this.id = id;
		this.username = username;
		this.password = password;
		this.position = position;
		this.contact = contact;
		this.salary = salary;
		this.gender = gender;
	}
	
	
	
	@Override
	public String toString() {
		return "Staff [id=" + id + ", username=" + username + ", password=" + password + ", position=" + position + ", contact=" + contact + ", gender=" + gender + "]";
	}
}
