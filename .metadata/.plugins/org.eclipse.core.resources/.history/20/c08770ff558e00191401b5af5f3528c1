package ex1;

public class Sfreelancer extends Employee {

	public Sfreelancer(String Stype, String ID, String name, float bSalary, int experience) {
		super(Stype, ID, name, bSalary, experience);
		// TODO Auto-generated constructor stub
	}

	public float monthlysalary(int hour) {
		// TODO Auto-generated method stub
		return getbSalary()*hour;
	}

}
