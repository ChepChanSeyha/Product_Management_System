package ex1;

public class Sfreelancer extends Employee {
	
	float monthlySalary = getbSalary();
	float annuallySaraly = 10*getExperience();

	public Sfreelancer(String Stype, String ID, String name, float bSalary, float monthlySalary, float annuallySaraly, int experience, int hour) {
		super(Stype, ID, name, bSalary, monthlySalary, annuallySaraly, experience, hour);
		// TODO Auto-generated constructor stub
		setMonthlySalary(monthlySalary);
		setAnnuallySaraly(annuallySaraly);
	}

}
